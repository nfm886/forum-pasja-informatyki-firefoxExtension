{
    "manifest_version": 2,
    "name": "Powiadomienia z Pasją",
    "description": "Dodatek pokazujący powiadomienia z Forum.Pasja-Informatyki.pl",
    "version": "2.1",
    "icons": {
      "16": "images/icon.png",
       "48": "images/icon-48x48.png",
      "128": "images/icon-128x128.png"
    },
    "options_ui": {
      "page": "settings.html"
    },
    "background": {
      "scripts": ["jquery.js","background.js"]
    },
    "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["*://*.pasja-informatyki.pl/", "storage"],
    "applications": {
      "gecko": {
        "id": "powiadomienia-z-pasja@forum.pasja-informatyki.pl",
        "strict_min_version": "42.0"
      }
    }
  }
