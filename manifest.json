{
    "manifest_version": 2,
    "name": "Powiadomienia z Pasją",
    "description": "Dodatek wyświetlający powiadomienia i obserwowane tagi z Forum.Pasja-Informatyki.pl",
    "version": "3.0",
    "icons": {
      "16": "/assets/images/icon.png",
       "48": "/assets/images/icon-48x48.png",
      "128": "/assets/images/icon-128x128.png"
    },
    "options_ui": {
      "page": "/views/settings.html"
    },
    "background": {
      "scripts": ["jquery.js","background.js"]
    },
    "browser_action": {
      "default_icon": "/assets/images/icon.png",
      "default_popup": "/views/popup.html"
    },
    "permissions": ["*://*.pasja-informatyki.pl/", "storage"],
    "applications": {
      "gecko": {
        "id": "{902aa5b5-4af1-42c3-85b3-d8d0d1fcc2f9}",
        "strict_min_version": "42.0"
      }
    }
  }
